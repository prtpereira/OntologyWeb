extends layout

block content
  script(src="/javascripts/jquery-3.2.1.min.js")
  script(src="/javascripts/ajax.js")
  div(class="w3-container")
    header(class="w3-container w3-center w3-teal")
      h1 Insira um indiv√≠duo na sua Base de Dados
  div(class="w3-row")
    div(class="w3-col s4 w3-padding")
        div(class="w3-container  w3-border w3-light-grey")
            header(class="w3-container")
                h4 Introduza os dados do seu individuo:
        div(class="w3-container w3-border")
            textarea(rows="5" cols="30" class="w3-input" placeholder="Nome..." name="inNome" id="inNome")
        div(class="w3-container w3-border")
            textarea(rows="2" cols="30" class="w3-input" placeholder="Ano..." name="inAno" id="inAno")
        div(class="w3-bar w3-pale-green")
          div(class="w3-bar-item w3-button" id="proc3") Inserir
        div(class="w3-col s12 w3-padding")
          div(class="w3-code w3-border" id="resultado3")
